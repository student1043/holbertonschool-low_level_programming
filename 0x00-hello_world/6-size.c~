#include <stdio.h>
#include <conio.h>
/**
 * main - Entry point
 * print message
 * Return: Always 0 (Success)
 */
int main()
{
  printf("Size of a char: %2d \n", sizeof(char));
  printf("Size of an int %2d \n", sizeof(int));
  printf("Size of a long int: %2d \n", sizeof(long int));
  printf("Size of a long long int: %2d \n", sizeof(float));
  printf("Size of a float: %2d \n", sizeof(long int));
  Return (0);
}
